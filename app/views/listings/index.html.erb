<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Beds</th>
      <th>Baths</th>
      <th>Sale price</th>
      <th>Rent price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.title %></td>
        <td><%= listing.description %></td>
        <td><%= listing.beds %></td>
        <td><%= listing.baths %></td>
        <td><%= listing.sale_price %></td>
        <td><%= listing.rent_price %></td>
        <td><%= link_to 'Show', listing %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
